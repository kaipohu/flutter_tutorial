    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_tutorial (staging)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_staging.dart",
            "args" : [
                "--flavor",
                "staging",
                "--verbose",
                "--dart-define=isEnvOverrideEnabled=true"
            ],
        },
        {
            "name": "flutter_tutorial (local)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_local.dart",
            "args" : [
                "--flavor",
                "local",
                "--verbose",
                "--dart-define=isEnvOverrideEnabled=true"
            ],
        },
        {
            "name": "flutter_tutorial (production)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args" : [
                "--flavor",
                "production",
                "--verbose",
                "--dart-define=isEnvOverrideEnabled=true"
            ],
        },
        {
            "name": "flutter_tutorial (production - profile)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_production.dart",
            "args" : [
                "--flavor",
                "production",
                "--verbose",
                "--dart-define=isEnvOverrideEnabled=true"
            ],
        },
    ]
}
